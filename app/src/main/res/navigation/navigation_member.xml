<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_member"
    app:startDestination="@id/memberAreaFragment">
    <fragment
        android:id="@+id/memberAreaFragment"
        android:name="com.example.buyhome_lcn.fragment_member_area.MemberAreaFragment"
        android:label="fragment_member_area"
        tools:layout="@layout/fragment_member_area" >
        <action
            android:id="@+id/action_memberAreaFragment_to_accountInfoFragment"
            app:destination="@id/accountInfoFragment" />
        <action
            android:id="@+id/action_memberAreaFragment_to_setAddressFragment"
            app:destination="@id/setAddressFragment" />
        <action
            android:id="@+id/action_memberAreaFragment_to_setStoreFragment"
            app:destination="@id/setStoreFragment" />
        <action
            android:id="@+id/action_memberAreaFragment_to_setPayMethodFragment"
            app:destination="@id/setPayMethodFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountInfoFragment"
        android:name="com.example.buyhome_lcn.fragment_member_area.AccountInfoFragment"
        android:label="fragment_account_info"
        tools:layout="@layout/fragment_account_info" >
        <action
            android:id="@+id/action_accountInfoFragment_to_setGenderFragment"
            app:destination="@id/setGenderFragment" />
        <action
            android:id="@+id/action_accountInfoFragment_to_setBirthdayFragment"
            app:destination="@id/setBirthdayFragment" />
        <action
            android:id="@+id/action_accountInfoFragment_to_setPhoneFragment"
            app:destination="@id/setPhoneFragment" />
    </fragment>
    <fragment
        android:id="@+id/setAddressFragment"
        android:name="com.example.buyhome_lcn.fragment_member_area.SetAddressFragment"
        android:label="fragment_set_address"
        tools:layout="@layout/fragment_set_address" />
    <fragment
        android:id="@+id/setStoreFragment"
        android:name="com.example.buyhome_lcn.fragment_member_area.SetStoreFragment"
        android:label="SetStoreFragment" />
    <fragment
        android:id="@+id/setPayMethodFragment"
        android:name="com.example.buyhome_lcn.fragment_member_area.SetPayMethodFragment"
        android:label="fragment_set_pay_method"
        tools:layout="@layout/fragment_set_pay_method" />
    <fragment
        android:id="@+id/setGenderFragment"
        android:name="com.example.buyhome_lcn.fragment_member_area.SetGenderFragment"
        android:label="fragment_set_gender" />
    <fragment
        android:id="@+id/setBirthdayFragment"
        android:name="com.example.buyhome_lcn.fragment_member_area.SetBirthdayFragment"
        android:label="fragment_set_birthday" />
    <fragment
        android:id="@+id/setPhoneFragment"
        android:name="com.example.buyhome_lcn.fragment_member_area.SetPhoneFragment"
        android:label="fragment_set_phone" />
</navigation>